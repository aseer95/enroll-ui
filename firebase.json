{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint",
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ]
  },
  "hosting": {
    "public": "dist/apps",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/admin/api/v1/agencies",
        "function": "agencies"
      },
      {
        "source": "/admin/api/v1/agencies/agency_staff",
        "function": "agencyStaff"
      },
      {
        "source": "/admin/api/v1/agencies/primary_agency_staff",
        "function": "primaryAgents"
      },
      {
        "source": "/assets/fonts/OpenSans-Bold.woff2",
        "destination": "/admin/assets/fonts/OpenSans-Bold.woff2",
        "type": 301
      },
      {
        "source": "/assets/fonts/OpenSans-SemiBold.woff2",
        "destination": "/admin/assets/fonts/OpenSans-SemiBold.woff2",
        "type": 301
      },
      {
        "source": "/assets/fonts/OpenSans-Regular.woff2",
        "destination": "/admin/assets/fonts/OpenSans-Regular.woff2",
        "type": 301
      },
      {
        "source": "/assets/icons/hbx-icons.svg",
        "destination": "/admin/assets/icons/hbx-icons.svg",
        "type": 301
      },
      {
        "source": "**",
        "destination": "/admin/index.html"
      }
    ]
  }
}
